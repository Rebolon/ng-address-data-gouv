{
  "name": "@rebolon/ng-address-data-gouv",
  "version": "0.0.4",
  "author": "Rebolon",
  "description": "A simple angular component to find French address from https://adresse.data.gouv.fr/api-doc",
  "repository": "https://github.com/Rebolon/ng-address-data-gouv",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project lib && ng build --output-hashing=none --project app && npm run concat",
    "concat": "mkdir -p dist\\app\\assets\\js && cp projects/app/src/index-for-docs.html dist/app/index.html && concat -o dist/app/assets/js/output.js dist/app/runtime.js dist/app/polyfills.js dist/app/main.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/elements": "^14.1.3",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "concat": "^1.0.3",
    "document-register-element": "^1.14.10",
    "ng-address-data-gouv": "^0.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@webcomponents/webcomponentsjs": "^2.6",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "typescript": "~4.7.2"
  }
}
